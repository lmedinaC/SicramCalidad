facebook-btn {\r\n    border-color: $facebook_color;\r\n    color: $facebook_color;\r\n    &:hover {\r\n      border-color: $facebook_color;\r\n      background: $facebook_color;\r\n    }\r\n  }\r\n\r\n  .github-btn {\r\n    border-color: $github_color;\r\n    color: $github_color;\r\n    &:hover {\r\n      border-color: $github_color;\r\n      background: $github_color;\r\n    }\r\n  }\r\n\r\n  .autocomplete-fix {\r\n    position: absolute;\r\n    visibility: hidden;\r\n    overflow: hidden;\r\n    opacity: 0;\r\n    width: 0;\r\n    height: 0;\r\n    left: 0;\r\n    top: 0;\r\n  }\r\n}\r\n\r\n.pop-out-enter-active,\r\n.pop-out-leave-active {\r\n  transition: all 0.5s;\r\n}\r\n\r\n.pop-out-enter,\r\n.pop-out-leave-active {\r\n  opacity: 0;\r\n  transform: translateY(24px);\r\n}\r\n</style>\r\n"]}]}